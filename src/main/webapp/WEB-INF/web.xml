<?xml version="1.0" encoding="UTF-8" ?>
<web-app version="3.1"
        xmlns="http://xmlns.jcp.org/xml/ns/javaee"
>


    <servlet>
        <servlet-name>initial</servlet-name>
        <servlet-class>com.byrneham.servlets.InitialServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>download</servlet-name>
        <servlet-class>com.byrneham.servlets.DownloadServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>initial</servlet-name>
        <url-pattern>/initial</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>download</servlet-name>
        <url-pattern>/download</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>mongodb</servlet-name>
        <servlet-class>com.byrneham.servlets.MongoDbServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>mongodb</servlet-name>
        <url-pattern>/mongodb</url-pattern>
    </servlet-mapping>



</web-app>